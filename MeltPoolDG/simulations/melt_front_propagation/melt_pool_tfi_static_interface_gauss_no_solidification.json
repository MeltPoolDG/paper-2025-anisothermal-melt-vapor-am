{
  "base": {
    "application name": "melt_front_propagation",
    "dimension": "2",
    "do print parameters": "false",
    "global refinements": "5",
    "problem name": "melt_pool",
    "verbosity level": "2",
    "fe": {
      "degree": "1"
    }
  },
  "time stepping": {
    "start time": "0.0",
    "time step size": "1e-4",
    "end time": "0.015",
    "max n steps": "150"
  },
  "laser": {
    "starting position": "0.0,0.0",
    "model": "interface_projection_regularized",
    "intensity profile": "Gauss",
    "power": "10.",
    "absorptivity gas": "0.5",
    "absorptivity liquid": "0.5",
    "do move": "false",
    "radius": "0.06e-3"
  },
  "simulation specific parameters": {
    "domain x min": "0.0",
    "domain x max": "0.6e-3",
    "domain z min": "-0.2e-3",
    "domain z max": "0.2e-3",
    "initial temperature": "1000.0"
  },
  "melt pool": {
    "mp set velocity to zero in solid": "false"
  },
  "material": {
    "gas": {
      "thermal conductivity": "10",
      "specific heat capacity": "0",
      "density": "74.303",
      "dynamic viscosity": "0.0006"
    },
    "liquid": {
      "thermal conductivity": "55.563",
      "specific heat capacity": "460.0",
      "density": "7850.0",
      "dynamic viscosity": "0.006"
    }
  },
  "heat": {
    "linear solver": {
      "rel tolerance": "1e-12",
      "preconditioner type": "ILU"
    },
    "nlsolve": {
      "residual tolerance": "1e-8"
    }
  },
  "problem specific": {
    "do heat transfer": "true",
    "do solidification": "true"
  },
  "output": {
    "directory": "output",
    "paraview": {
      "filename": "melt_pool_tfi_static_interface_gauss",
      "enable": "true"
    }
  },
  "level set": {
    "reinitialization": {
      "linear solver": {
        "do matrix free": "true"
      },
      "enable": "true",
      "max n steps": "5",
      "type": "olsson2007",
      "interface thickness parameter": {
        "type": "proportional_to_cell_size",
        "val": "0.7107"
      }
    },
    "curvature": {
      "linear solver": {
        "do matrix free": "true"
      },
      "filter parameter": "0.5"
    },
    "normal vector": {
      "linear solver": {
        "do matrix free": "true"
      },
      "filter parameter": "0.5"
    },
    "advection diffusion": {
      "linear solver": {
        "do matrix free": "true"
      }
    }
  },
  "flow": {
    "adaflo": {
      "Navier-Stokes": {
        "physical type": "incompressible",
        "dimension": "2",
        "global refinements": "0",
        "velocity degree": "2",
        "Solver": {
          "linearization scheme": "coupled implicit Newton",
          "NL max iterations": "10",
          "NL tolerance": "1.e-9",
          "lin max iterations": "30",
          "lin tolerance": "1.e-4",
          "lin relative tolerance": "1",
          "lin velocity preconditioner": "ilu scalar",
          "lin pressure mass preconditioner": "ilu",
          "lin its before inner solvers": "50"
        }
      },
      "Time stepping": {
        "scheme": "bdf_2"
      },
      "Output options": {
        "output verbosity": "2",
        "output walltimes": "0"
      }
    }
  }
}