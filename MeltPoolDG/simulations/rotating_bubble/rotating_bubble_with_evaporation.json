{
  "base": {
    "problem name": "level_set_with_evaporation",
    "application name": "rotating_bubble",
    "dimension": "2",
    "global refinements": "7",
    "fe": {
      "degree": "1"
    }
  },
  "time stepping": {
    "start time": "0.0",
    "end time": "1.5707963267948966",
    "time step size": "0.01"
  },
  "adaptive meshing": {
    "do amr": "true",
    "do not modify boundary cells": "false",
    "upper perc to refine": "     0.15",
    "lower perc to coarsen": "0.15",
    "n initial refinement cycles": "3",
    "max grid refinement level": "7",
    "every n step": "1"
  },
  "evaporation": {
    "analytical": {
      "function": "1.0e-4"
    }
  },
  "output": {
    "directory": "rotating_bubble_evapor/",
    "paraview": {
      "filename": "solution_evaporation",
      "enable": "true"
    }
  },
  "level set": {
    "reinitialization": {
      "linear solver": {
        "do matrix free": "true"
      },
      "enable": "true",
      "max n steps": "5",
      "type": "olsson2007",
      "implementation": "meltpooldg",
      "interface thickness parameter": {
        "type": "absolute_value",
        "val": "0.01"
      }
    },
    "curvature": {
      "linear solver": {
        "do matrix free": "true"
      },
      "implementation": "meltpooldg"
    },
    "normal vector": {
      "linear solver": {
        "do matrix free": "true"
      },
      "implementation": "meltpooldg"
    },
    "advection diffusion": {
      "linear solver": {
        "do matrix free": "true"
      }
    }
  }
}