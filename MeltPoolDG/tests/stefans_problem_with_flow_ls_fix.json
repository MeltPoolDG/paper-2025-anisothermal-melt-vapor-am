{
  "base": {
    "problem name": "melt_pool",
    "application name": "stefans_problem_with_flow",
    "dimension": "2",
    "global refinements": "5",
    "do print parameters": "false",
    "fe": {
      "degree": "1"
    }
  },
  "time stepping": {
    "start time": "0.0",
    "end time": "0.002",
    "time step size": "0.0005"
  },
  "material": {
    "gas": {
      "thermal conductivity": "1e-16",
      "density": "0.01",
      "dynamic viscosity": "1e-6"
    },
    "liquid": {
      "thermal conductivity": "1e-16",
      "density": "1.0",
      "dynamic viscosity": "1e-6"
    },
    "two phase fluid properties transition type": "consistent_with_evaporation"
  },
  "evaporation": {
    "analytical": {
      "function": "1.e-2"
    },
    "evaporative cooling": {
      "enable": "false"
    },
    "evaporative dilation rate": {
      "enable": "true"
    }
  },
  "problem specific": {
    "do heat transfer": "true",
    "do advect level set": "false"
  },
  "output": {
    "do user defined postprocessing": "true",
    "paraview": {
      "enable": "false"
    }
  },
  "level set": {
    "reinitialization": {
      "linear solver": {
        "do matrix free": "true"
      },
      "enable": "true",
      "max n steps": "5",
      "type": "olsson2007",
      "interface thickness parameter": {
        "type": "absolute_value",
        "val": "0.02"
      }
    },
    "curvature": {
      "filter parameter": "0.0",
      "linear solver": {
        "do matrix free": "true"
      },
      "implementation": "meltpooldg"
    },
    "normal vector": {
      "filter parameter": "0.5",
      "linear solver": {
        "do matrix free": "true"
      },
      "implementation": "meltpooldg"
    },
    "advection diffusion": {
      "linear solver": {
        "do matrix free": "true"
      }
    }
  },
  "flow": {
    "adaflo": {
      "Navier-Stokes": {
        "physical type": "incompressible",
        "dimension": "2",
        "global refinements": "0",
        "velocity degree": "2",
        "formulation convective term momentum balance": "convective",
        "Solver": {
          "linearization scheme": "coupled implicit Newton",
          "NL max iterations": "10",
          "NL tolerance": "1.e-9",
          "lin max iterations": "30",
          "lin tolerance": "1.e-4",
          "lin velocity preconditioner": "ilu",
          "lin its before inner solvers": "50"
        }
      },
      "Time stepping": {
        "scheme": "bdf_2"
      },
      "Output options": {
        "output verbosity": "0",
        "output walltimes": "0"
      }
    },
    "gravity": "0."
  }
}